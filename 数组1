class Solution:
    def getConcatenation(self, nums: List[int]) -> List[int]:
        ans = nums + nums
        return ans
class Solution:
    def shuffle(self, nums: List[int], n: int) -> List[int]:
        a=[]
        for i in range(n):
            a.append(nums[i])
            a.append(nums[i+n])
        return a
class Solution:
    def findMaxConsecutiveOnes(self, nums: List[int]) -> int:
        a,maxa = 0,0
        for i in nums:
            if i == 1: 
                a += 1
                maxa=max(a,maxa)
